# DNS Options
port=0
interface=ens192,lo


# DHCP Options
dhcp-range=10.0.0.0,proxy
dhcp-option=vendor:PXEClient,6,2b
dhcp-no-override


# PXE
dhcp-boot=pxelinux.0,{{ pxe_server_name }},{{ pxe_server_ip }}


#pxe-prompt="Press F8 for menu.", 10
pxe-service=X86PC, "void.lab", pxelinux


# TFTP Options
enable-tftp
tftp-root=/var/lib/tftpboot